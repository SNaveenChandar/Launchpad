<mvc:View
    controllerName="com.chs.launchpad.controller.Home"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.f"
>
    <Page
        id="page"
        showHeader="false"
    >
        <content>
            <f:ShellBar
                id="shb"
                title="Launchpad"
                homeIcon="./logo.png"
                showCopilot="false"
                showSearch="false"
                showNotifications="false"
                notificationsNumber=""
            >
                <f:profile>
                    
                </f:profile>
            </f:ShellBar>

            <IconTabBar
                id="idIconTabBarNoIcons"
                expanded="{device>/isNoPhone}"
                class="sapUiResponsiveContentPadding"
            >
                <items>
                <IconTabFilter
                        id="it30"
                        text="Renewables Application"
                        key="T"
                        content="{/Transactional}"
                    >
                        <GenericTile
                            class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                            header="{applicationName}"
                            press="onPressTile(${url})"
                        />
                    </IconTabFilter>
                <IconTabFilter
                        id="it3"
                        text="Master Configuration â€“ General"
                        key="TS"
                        content="{/Business}"
                    >
                        <GenericTile
                            class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                            header="{applicationName}"
                            press="onPressTile(${url})"
                        />
                    </IconTabFilter>

                    <IconTabFilter
                        id="it2"
                        text="Renewable Master Configuration - RFS2 specific "
                        key="TS"
                        content="{/RFS2Specific}"
                    >
                        <GenericTile
                            class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                            header="{applicationName}"
                            press="onPressTile(${url})"
                        />
                    </IconTabFilter>
                     <IconTabFilter
                        id="it29"
                        text="Renewable Technical Configuration"
                        key="TS"
                        content="{/Technical}"
                    >
                        <GenericTile
                            class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                            header="{applicationName}"
                            press="onPressTile(${url})"
                        />
                    </IconTabFilter>
                    
                </items>
            </IconTabBar>
        </content>
    </Page>
</mvc:View>
